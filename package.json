{
  "name": "nextfire-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "extract": "formatjs extract",
    "extract:i18n": "formatjs extract '{pages,components,sections}/**/*.{js,ts,tsx}' --ignore='**/*.d.ts' --out-file content/locales/en-US.json --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile:i18n": "formatjs compile-folder --ast --format simple content/locales content/compiled-locales",
    "i18n": "yarn extract:i18n && yarn compile:i18n"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^1.0.6",
    "@mui/material": "^5.9.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "firebase": "^9.8.3",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "moment": "^2.29.4",
    "next": "12.2.0",
    "next-themes": "^0.2.0",
    "postcss": "^8.4.14",
    "postmark": "^3.0.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "^5.0.3",
    "react-hook-form": "^7.32.2",
    "react-hot-toast": "^2.2.0",
    "react-intl": "^6.2.5",
    "react-markdown": "^8.0.3",
    "react-redux": "^8.0.2",
    "react-transition-group": "^4.4.2",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "tailwindcss": "^3.1.4",
    "twilio": "^3.79.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.1.10",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "typescript": "^4.7.4"
  }
